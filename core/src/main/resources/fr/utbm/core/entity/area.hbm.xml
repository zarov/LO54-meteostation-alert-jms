<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="fr.utbm.core.entity">
    <class name="Area" table="area">
        <id name="id" column="Are_id">
            <generator class="increment"/>
        </id>
        <property name="label" column="Are_Label" type="string" not-null="true" />
        <property name="road" column="Are_Road" type="string" not-null="true" />
    </class>
</hibernate-mapping>	