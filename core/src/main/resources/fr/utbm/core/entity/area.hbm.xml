<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="fr.utbm.core.entity">
    <class name="Area" table="area">
        <id name="id" type="int" column="Are_Id">
        	<generator class="native" />
        </id>
        <set name="stations" table="station" cascade="all">
        	<key column="Are_Id" />
        	<one-to-many class="Station" />
        </set>
        <property name="label" column="Are_Label" type="string" not-null="true" />
        <property name="road" column="Are_Road" type="string" not-null="true" />
    </class>
</hibernate-mapping>	